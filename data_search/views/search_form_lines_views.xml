<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="search_view" model="ir.ui.view">
        <field name="name">search_view</field>
        <field name="model">search.form.lines</field>
        <field name="arch" type="xml">
            <form string="Search Form">
                <sheet>
                    <notebook>
                        <group>
                        <field name="search_id"/>
                        <field name="model_id"/>
                        <field name="field_id"/>
                    </group>
                    </notebook>

                </sheet>
            </form>
        </field>
    </record>
</odoo>
