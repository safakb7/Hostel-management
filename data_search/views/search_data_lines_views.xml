<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_search_data_line_tree" model="ir.ui.view">
        <field name="name">search.data.line.tree</field>
        <field name="model">search.data.line</field>
        <field name="arch" type="xml">
            <tree string="Search Data Lines">
                <field name="search_id"/>
                <field name="record_data"/>
            </tree>
        </field>
    </record>
    <record id="action_search_data_line" model="ir.actions.act_window">
        <field name="name">Search Data Lines</field>
        <field name="res_model">search.data.lines</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
